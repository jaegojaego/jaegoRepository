<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jaego.web.DAO.SellerMapper">

	<insert id="addSeller" parameterType="Seller">
		insert into
			Seller(
				sellerCRN
				,sellerId
				,sellerPw
				,sellerName
				,sellerShopName
				,sellerPhone
				,sellerEmail
				,sellerBEA
				,sellerTOB
				,sellerShopOimg
				,sellerShopSimg
				,sellerLat
				,sellerLong
			)values(
				#{sellerCRN}
				,#{sellerId}
				,#{sellerPw}
				,#{sellerName}
				,#{sellerShopName}
				,#{sellerPhone}
				,#{sellerEmail}
				,#{sellerBEA}
				,#{sellerTOB}
				,#{sellerShopOimg}
				,#{sellerShopSimg}
				,#{sellerLat}
				,#{sellerLong}			
			)
	</insert>
	
</mapper>
